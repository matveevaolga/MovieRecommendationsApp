<Window x:Class="MovieRecommendationsApp.MVVM.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MovieRecommendationsApp.MVVM.View"
        mc:Ignorable="d" WindowState="Maximized"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        Title="Authorization" Height="900" Width="1500"
        Loaded="NewLogInProcessing">
    <Grid Background="#4F4557">
        <Border Style="{StaticResource MainBorder}">
            <Label Style="{StaticResource TitleLabel}" Content="Вход выполнен" HorizontalAlignment="Center"
                   VerticalAlignment="Center"/>
        </Border>
    </Grid>
</Window>
